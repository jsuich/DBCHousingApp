<div class="newUserWrapper">
  <div id="form">
    <h3>Fill in your info</h3>
  <%= form_for(:user, url: users_path) do |f|%>

  <%= f.text_field :name, :placeholder => "Mike Perry" %>
  <%= f.text_field :cohort, :placeholder => "Nighthawks 2013" %>
  <%= f.email_field :email, :placeholder => "thefuturemrperry@mikelikesgpop.com" %>
  <%= f.telephone_field :phone_number, :placeholder => "XXX-XXX-XXXX" %>

  <%= f.text_field :street_address, :placeholder => "123 Main St., Apt. #" %>
  <%= f.text_field :city, :placeholder => "Chicago" %>
  <%= f.text_field :state, :placeholder => "IL" %>
  <%= f.text_field :zip, :placeholder => "60606" %>
  <%= f.hidden_field :json %>
    <button class="check">Check Address</button>
    <p class="error"> </p>

    <div class="yesno">
    <p>Does the map show a marker at your correct address?</p>
    <button class="yes">yes</button> <button class="no">no</button>
    </div>

  <%=f.submit "submit"%>

  <% end %>
  </div>
  <div id="map-canvas"></div>

</div>
